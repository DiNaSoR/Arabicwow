    <Frame name="ArabicwowDecorationFrameTemplate" virtual="true">
        <Size>
            <AbsDimension x="148" y="16"/>
        </Size>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parentTopRight" file="Interface\Common\Common-Input-Border">
                    <Size>
                        <AbsDimension x="8" y="8"/>
