                    <TexCoords left="0.9375" right="1.0" top="0.375" bottom="0.625"/>
                </Texture>
                <Texture name="$parentRight" file="Interface\Common\Common-Input-Border">
                    <Size>
                        <AbsDimension x="8" y="0"/>
