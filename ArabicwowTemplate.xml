                    </Anchors>
                    <TexCoords left="0" right="0.0625" top="0.25" bottom="0.375"/>
                </Texture>
                <Texture name="$parentTopMiddle" file="Interface\Common\Common-Input-Border">
                    <Size>
                        <AbsDimension x="100" y="8"/>
